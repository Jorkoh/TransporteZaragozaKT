    <style name="CustomTabStyle" parent="Widget.MaterialComponents.TabLayout">
        <item name="tabTextColor">@color/tab_text_color</item>
        <item name="tabIconTint">?colorSecondary</item>
        <item name="tabRippleColor">?colorSecondary</item>
        <item name="tabIndicatorColor">?colorSecondary</item>
    </style>

    <style name="CustomTimePickerTheme" parent="@android:style/Widget.Material.TimePicker">
        <item name="android:headerBackground">?colorSecondary</item>
        <item name="android:numbersBackgroundColor">?colorPrimaryVariant</item>
        <item name="android:numbersSelectorColor">?colorPrimary</item>
        <item name="android:numbersTextColor">?colorOnPrimary</item>
        <item name="android:numbersInnerTextColor" tools:targetApi="m">?colorOnPrimary</item>
    </style>

    <style name="BaseTheme.CustomBottomNavigationViewText" parent="TextAppearance.MaterialComponents.Caption">
        <item name="android:textSize">10sp</item>
    </style>

    <style name="BaseTheme.PreferenceThemeOverlay" parent="@style/PreferenceThemeOverlay">
        <item name="android:layout">@layout/more_destination</item>
    </style>

    <style name="BaseTheme" parent="Theme.MaterialComponents.NoActionBar">
        <item name="android:scrollbars">none</item>
        <item name="android:fontFamily">@font/manrope_medium</item>
        <item name="android:timePickerStyle">@style/CustomTimePickerTheme</item>
        <item name="tabStyle">@style/CustomTabStyle</item>
        <item name="preferenceTheme">@style/BaseTheme.PreferenceThemeOverlay</item>
        <item name="colorBackgroundFloating">?android:colorBackground</item>
        <item name="android:textColorPrimary">?colorOnBackground</item>
        <item name="android:textColorSecondary">?colorOnBackground</item>
        <item name="android:textColorTertiary">?colorOnBackground</item>
        <item name="colorAccent">?colorSecondary</item>
        <item name="colorSurface">?android:colorBackground</item>
        <item name="colorOnSurface">?colorOnBackground</item>
        <item name="md_corner_radius">12dp</item>
        <item name="md_color_button_text">?colorOnBackground</item>
    </style>

